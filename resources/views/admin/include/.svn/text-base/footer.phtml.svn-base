function webkitNotify(mobile,callee,name,id)
{
	if(window.webkitNotifications)
	{
		if(window.webkitNotifications.checkPermission() == 0)
		{
			if(mobile=='801'||mobile=='802'||mobile=='803')
			{
				var title = mobile+' 呼叫：'+name+' 号码：'+callee;
			}else
			{
				var title = name+'：'+mobile+' 来电 接收号码'+callee;
			}
			var notification_test = window.webkitNotifications.createNotification("http://i2.umivi.net/avatar/46/6187446m.jpg",title, ' 点击查看更多资料');
			notification_test.display = function() {}
			notification_test.onerror = function() {}
			notification_test.onclose = function() {}
			notification_test.onclick = function() {this.cancel();window.open('/caller/index/?mobile='+mobile+'&callee='+callee+'&callcenterid='+id);}

			notification_test.replaceId = 'callin' //sn.substring(0,4);

			notification_test.show();
		}else{
			window.webkitNotifications.requestPermission();//function(){return webkitNotify();}
		}
	}
}

function requestwebkitNotificationsPermission()
{
	if(window.webkitNotifications)
	{
		if(window.webkitNotifications.checkPermission() != 0)
		{
			window.webkitNotifications.requestPermission();
		}
	}
}
function showRequestWindow()
{
	if(window.webkitNotifications)
	{
		if(window.webkitNotifications.checkPermission() != 0)
		{
