<script>

function favmenu()
{
	var title=prompt("您将收藏该页面到菜单收藏夹\n请为该页面输入一个好记的名称");
	var url=location.href.replace(location.origin,'');
	if(!title)
	{
		alert('未填写名称');
		return;
	}

	$.ajax({
		type:"POST",
		data:{url:url,title:title},
		url:"/favmenu/add/",
		dataType:"html",
		cache: false,
		success:function(data){
			alert(data);
			loadfavmenu();
			//自己写回调
		}
	});

}
function loadfavmenu()
{
	$('#float_favmenu_div').load('/favmenu/usermenu');
	if(getCookie('favmenu_hide')==1)
	{
		favmenu_hide();
	}else
	{
		favmenu_show();
	}
}
function favmenu_del(id, title)
{
	if(!confirm('确定要删除 “'+title+'” 吗？'))
	{
		return;
	}
	$.ajax({
		type:"POST",
		data:{id:id},
		url:"/favmenu/del/",
		dataType:"html",
		cache: false,
		success:function(data){
			alert(data);
			loadfavmenu();
			//自己写回调
		}
	});
}
function favmenu_show()
{
	$('#favmenu_normal').show();
	$('#favmenu_hidden').hide();
	setCookie('favmenu_hide',0,'/');
}
function favmenu_hide()
{
	$('#favmenu_normal').hide();
	$('#favmenu_hidden').show();
	setCookie('favmenu_hide',1,'/');
}
loadfavmenu();

</script>
